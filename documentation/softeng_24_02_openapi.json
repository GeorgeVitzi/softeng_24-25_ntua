          }
        ],
        "responses": {
          "200": {
            "description": "Charges by visiting operators",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ChargesByResponse"
                }
              }
            }
          },
          "400": {
