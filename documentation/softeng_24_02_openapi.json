{
  "openapi": "3.0.3",
  "info": {
    "title": "Toll Management API",
    "description": "API for managing toll station passes and related data.",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://api.example.com",
      "description": "Production Server"
    },
    {
      "url": "https://staging.example.com",
      "description": "Staging Server"
    }
  ],
  "paths": {
    "/tollStationPasses/{tollStationID}/{date_from}/{date_to}": {
      "get": {
        "summary": "Get passes for a specific toll station within a time range",
        "parameters": [
          {
            "name": "tollStationID",
            "in": "path",
            "required": true,
            "description": "ID of the toll station",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "date_from",
            "in": "path",
            "required": true,
            "description": "Start date (format: YYYYMMDD)",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "date_to",
            "in": "path",
            "required": true,
            "description": "End date (format: YYYYMMDD)",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "format",
            "in": "query",
            "required": false,
            "description": "Response format (json or csv)",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "List of passes",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/TollStationPassesResponse"
                }
              }
            }
          },
          "400": {
            "description": "Invalid request"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    },
